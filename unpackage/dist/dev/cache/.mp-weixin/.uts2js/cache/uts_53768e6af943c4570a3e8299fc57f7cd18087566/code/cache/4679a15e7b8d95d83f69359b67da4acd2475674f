{"code":"import { __read } from \"tslib\";\nimport { computed } from 'vue';\nimport { useComponentColor, useNamespace } from '../../../../hooks';\nimport { isEmpty } from '../../../../utils';\nimport { useCheckboxCommonProps } from './use-checkbox-common-props';\nexport const useCheckboxCustomStyle = (props) => {\n    const ns = useNamespace('checkbox');\n    const _a = useCheckboxCommonProps(props), activeColor = _a.activeColor, disabled = _a.disabled, maxDisabled = _a.maxDisabled, size = _a.size, border = _a.border, checkedShape = _a.checkedShape;\n    // 解析颜色\n    const _b = __read(useComponentColor(activeColor, 'bg'), 2), bgColorClass = _b[0], bgColorStyle = _b[1];\n    const _c = __read(useComponentColor(activeColor, 'text'), 2), textColorClass = _c[0], textColorStyle = _c[1];\n    const _d = __read(useComponentColor(activeColor, 'border'), 2), borderColorClass = _d[0], borderColorStyle = _d[1];\n    // 复选框所属类\n    const checkboxClass = computed(() => {\n        return (selected) => {\n            const cls = [ns.b()];\n            // 禁止选择\n            if (disabled.value || maxDisabled.value)\n                cls.push(ns.m('disabled'));\n            // 设置尺寸\n            if (size.value)\n                cls.push(ns.m(size.value));\n            // 激活样式\n            if (selected) {\n                cls.push(ns.m('selected'));\n                if (textColorClass.value)\n                    cls.push(textColorClass.value);\n            }\n            // 设置激活时的边框颜色\n            if (border.value) {\n                cls.push('tn-border');\n                if (selected && borderColorClass.value)\n                    cls.push(borderColorClass.value);\n                else\n                    cls.push('tn-gray-disabled_border');\n            }\n            else {\n                cls.push(ns.m('no-border'));\n            }\n            if (props.customClass)\n                cls.push(props.customClass);\n            return cls.join(' ');\n        };\n    });\n    // 复选框所属样式\n    const checkboxStyle = computed(() => {\n        return (selected) => {\n            const style = {};\n            // 设置激活时的颜色\n            if (selected) {\n                if (border.value && !borderColorClass.value)\n                    style.borderColor =\n                        borderColorStyle.value || 'var(--tn-color-primary)';\n                if (!textColorClass.value) {\n                    style.color = textColorStyle.value || 'var(--tn-color-primary)';\n                }\n            }\n            if (!isEmpty(props.customStyle))\n                Object.assign(style, props.customStyle);\n            return style;\n        };\n    });\n    // 复选框选框所属类\n    const checkboxCheckedBoxClass = computed(() => {\n        return (selected) => {\n            const cls = [ns.e('checked-box')];\n            // 复选框选框的形状\n            if (checkedShape.value)\n                cls.push(ns.em('checked-box', checkedShape.value));\n            if (selected || props.indeterminate) {\n                cls.push(ns.em('checked-box', 'selected'));\n                if (bgColorClass.value)\n                    cls.push(bgColorClass.value);\n            }\n            else {\n                cls.push('tn-border tn-gray-disabled_border');\n            }\n            return cls.join(' ');\n        };\n    });\n    // 复选框选框所属样式\n    const checkboxCheckedBoxStyle = computed(() => {\n        return (selected) => {\n            const style = {};\n            if ((selected || props.indeterminate) && !bgColorClass.value) {\n                style.backgroundColor = bgColorStyle.value || 'var(--tn-color-primary)';\n            }\n            return style;\n        };\n    });\n    return {\n        ns,\n        checkboxClass,\n        checkboxStyle,\n        checkboxCheckedBoxClass,\n        checkboxCheckedBoxStyle,\n    };\n};\n//# sourceMappingURL=D:/DEV/Project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/checkbox/src/composables/checkbox-custom.js.map","references":["D:/