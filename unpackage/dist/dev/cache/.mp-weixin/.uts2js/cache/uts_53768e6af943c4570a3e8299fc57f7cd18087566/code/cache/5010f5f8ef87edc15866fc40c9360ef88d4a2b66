{"code":"import { __read } from \"tslib\";\nimport { computed, inject } from 'vue';\nimport { tabsContextKey } from '../../../../tokens';\nimport { useComponentColor, useNamespace } from '../../../../hooks';\nimport { formatDomSizeValue, isEmptyVariableInDefault } from '../../../../utils';\nexport const useTabsItemCustomStyle = (props, isActive) => {\n    const ns = useNamespace('tabs-item');\n    const tabsContext = inject(tabsContextKey);\n    const normalColor = computed(() => { return props.color || (tabsContext === null || tabsContext === void 0 ? void 0 : tabsContext.color); });\n    const activeColor = computed(() => { return props.activeColor || (tabsContext === null || tabsContext === void 0 ? void 0 : tabsContext.activeColor); });\n    const activeBold = computed(() => {\n        return isEmptyVariableInDefault(tabsContext === null || tabsContext === void 0 ? void 0 : tabsContext.activeBold, true);\n    });\n    const activeFontSize = computed(() => { return props.activeFontSize || (tabsContext === null || tabsContext === void 0 ? void 0 : tabsContext.activeFontSize); });\n    // 解析颜色\n    const _a = __read(useComponentColor(normalColor, 'text'), 2), textColorClass = _a[0], textColorStyle = _a[1];\n    const _b = __read(useComponentColor(activeColor, 'text'), 2), activeTextColorClass = _b[0], activeTextColorStyle = _b[1];\n    // tabsItem对应的类\n    const tabsItemClass = computed(() => {\n        const cls = [ns.b()];\n        // 设置颜色\n        if (isActive.value) {\n            if (activeTextColorClass.value) {\n                cls.push(activeTextColorClass.value);\n            }\n            if (activeBold.value) {\n                cls.push(ns.m('bold'));\n            }\n        }\n        else {\n            if (textColorClass.value) {\n                cls.push(textColorClass.value);\n            }\n        }\n        // 设置可以滚动\n        if (tabsContext === null || tabsContext === void 0 ? void 0 : tabsContext.scroll)\n            cls.push(ns.m('scroll'));\