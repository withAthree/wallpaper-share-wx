{"code":"import { __read } from \"tslib\";\nimport { computed, toRef } from 'vue';\nimport { formatDomSizeValue, isEmpty } from '../../../../utils';\nimport { useNamespace } from '../../../../hooks/use-namespace';\nimport { useComponentColor } from '../../../../hooks/use-component-color';\nimport { useComponentSize } from '../../../../hooks/use-component-size';\nexport const useIcon = (props) => {\n    const ns = useNamespace('icon');\n    // 解析图标颜色\n    const _a = __read(useComponentColor(toRef(props, 'color'), 'text'), 3), colorClass = _a[0], colorStyle = _a[1], updateColor = _a[2];\n    // 解析透明图标背景\n    const _b = __read(useComponentColor(toRef(props, 'transparentBg'), 'bg'), 1), transparentBgClass = _b[0];\n    // 解析图标尺寸\n    const sizeType = useComponentSize(props.size).sizeType;\n    // 判断是图片还是图鸟内置的图标\n    const isImg = computed(() => { return !!(props === null || props === void 0 ? void 0 : props.name) && props.name.includes('/'); });\n    // 图标对应的类名\n    const iconClass = computed(() => {\n        const cls = [];\n        cls.push(ns.b());\n        // 判断图标是否是图片\n        if (isImg.value) {\n            cls.push(ns.m('image'));\n        }\n        else {\n            // 设置图标颜色类型\n            if (props.type)\n                cls.push(`tn-type-${props.type}_text`);\n            // 判断是否设置透明图标背景\n            if (props.transparent) {\n                cls.push('tn-text-transparent', transparentBgClass.value);\n            }\n            else {\n                // 设置图标颜色\n                if (colorClass.value)\n                    cls.push(colorClass.value);\n            }\n            // 设置图标加粗\n            if (props.bold)\n                cls.push('tn-text-bold');\n        }\n        // 设置图标尺寸\n        if (sizeType.value === 'inner')\n            cls.push(ns.m(props.size));\n        if (props.customClass)\n            cls.push(props.customClass);\n        return cls.join(' ');\n    });\n    // 图标对应的样式\n    const iconStyle = computed(() => {\n        const style = {};\n        // 判断图标是否是图片\n        if (isImg.value) {\n            // 如果设置了size，则设置图片的宽高\n            if (sizeType.value === 'custom' && props.size) {\n                style.width = style.height = formatDomSizeValue(props.size);\n            }\n        }\n        else {\n            // 设置图标颜色\n            if (colorStyle.value)\n                style.color = colorStyle.value;\n            // 设置图标大小\n            if (sizeType.value === 'custom' && props.size)\n                style.fontSize = formatDomSizeValue(props.size);\n        }\n        // 设置垂直方向上的偏移量\n        if (props.offsetTop)\n            style.transform = `translateY(${formatDomSizeValue(props.offsetTop)})`;\n        if (!isEmpty(props.customStyle))\n            Object.assign(style, props.customStyle);\n        return style;\n    });\n    return {\n        isImg,\n        iconClass,\n        iconStyle,\n        updateColor,\n    };\n};\n//# sourceMappingURL=D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/icon/src/composables/icon-custom.js.map","references":["D:/Development/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/utils/index.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/hooks/use-namespace/index.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/hooks/use-component-color/index.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/hooks/use-component-size/index.ts","D:/Development/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/icon/src/icon.ts"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"icon-custom.js\",\"sourceRoot\":\"\",\"sources\":[\"icon-custom.ts\"],\"names\":[],\"mappings\":\";AAAA,OAAO,EAAE,QAAQ,EAAE,KAAK,EAAE,MAAM,KAAK,CAAA;AACrC,OAAO,EAAE,kBAAkB,EAAE,OAAO,EAAE