{"code":"import { __read } from \"tslib\";\nimport { computed, toRef } from 'vue';\nimport { useComponentColor, useComponentSize, useNamespace, } from '../../../../hooks';\nimport { formatDomSizeValue } from '../../../../utils';\nexport const useLoadingCustomStyle = (props) => {\n    const ns = useNamespace('loading');\n    // 解析颜色\n    const _a = __read(useComponentColor(toRef(props, 'color'), 'bg'), 3), colorClass = _a[0], colorStyle = _a[1], updateColor = _a[2];\n    // 解析尺寸\n    const sizeType = useComponentSize(props.size).sizeType;\n    // 加载动画所属类\n    const loadingClass = computed(() => {\n        const cls = [];\n        cls.push(ns.b());\n        // 设置尺寸\n        if (props.size && sizeType.value === 'inner')\n            cls.push(ns.m(props.size));\n        return cls.join(' ');\n    });\n    // 加载动画所属样式\n    const loadingStyle = computed(() => {\n        const style = {};\n        // 设置尺寸\n        if (props.size && sizeType.value === 'custom')\n            style.width = style.height = formatDomSizeValue(props.size);\n        return style;\n    });\n    // 加载内容所属类\n    const loadingContentClass = computed(() => {\n        const cls = [];\n        cls.push(ns.b());\n        // 是否开启动画\n        if (props.animation)\n            cls.push(ns.m('animation'));\n        return cls.join(' ');\n    });\n    // 加载内容所属样式\n    const loadingContentStyle = computed(() => {\n        const style = {};\n        // 加载颜色类型\n        if (props.type)\n            style['--loading-color'] = `var(--tn-color-${props.type})`;\n        // 加载内容颜色\n        if (props.color && colorClass.value) {\n            const color = props.color.replace('tn-', '');\n            style['--loading-color'] = `var(--tn-color-${color})`;\n        }\n        // 加载内容颜色\n        if (colorStyle.value)\n            style['--loading-color'] = colorStyle.value;\n        // 设置动画执行时间\n        if (props.duration)\n            style.animationDuration = `${props.duration}s`;\n        // 设置动画执行时间函数\n        if (props.mode === 'circle' || props.mode === 'semicircle') {\n            if (props.timeFunction)\n                style.animationTimingFunction = props.timeFunction;\n        }\n        return style;\n    });\n    return {\n        ns,\n        loadingClass,\n        loadingStyle,\n        loadingContentClass,\n        loadingContentStyle,\n        updateColor,\n    };\n};\n//# sourceMappingURL=D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/loading/src/composables/loading-custom.js.map","references":["D:/Development/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/hooks/index.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/utils/index.ts","D:/Development/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/loading/src/loading.ts"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"loading-custom.js\",\"sourceRoot\":\"\",\"sources\":[\"loading-custom.ts\"],\"names\":[],\"mappings\":\";AAAA,OAAO,EAAE,QAAQ,EAAE,KAAK,EAAE,MAAM,KAAK,CAAA;AACrC,OAAO,EACL,iBAAiB,EACjB,gBAAgB,EAChB,YAAY,GACb,MAAM,mBAAmB,CAAA;AAC1B,OAAO,EAAE,kBAAkB,EAAE,MAAM,mBAAmB,CAAA;AAKtD,MAAM,CAAC,MAAM,qBAAqB,GAAG,CAAC,KAAmB;IACvD,MAAM,EAAE,GAAG,YAAY,CAAC,SAAS,CAAC,CAAA;IAElC,OAAO;IACD,MAAA,KAAA,OAAwC,iBAAiB,CAC7D,KAAK,CAAC,KAAK,EAAE,OAAO,CAAC,EACrB,IAAI,CACL,IAAA,EAHM,UAAU,QAAA,EAAE,UAAU,QAAA,EAAE,WAAW,QAGzC,CAAA;IAED,OAAO;IACC,MAAA,QAAQ,GAAK,gBAAgB,CAAC,KAAK,CAAC,IAAI,CAAC,SAAjC,CAAiC;IAEjD,UAAU;IACV,MAAM,YAAY,GAAG,QAAQ,CAAS;QACpC,MAAM,GAAG,GAAa,EAAE,CAAA;QACxB,GAAG,CAAC,IAAI,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAA;QAEhB,OAAO;QACP,IAAI,KAAK,CAAC,IAAI,IAAI,QAAQ,CAAC,KAAK,KAAK,OAAO;YAC1C,GAAG,CAAC,IAAI,CAAC,EAAE,CAAC,CAAC,CAAC,KAAK,CAAC,IAAc,CAAC,CAAC,CAAA;QAEtC,OAAO,GAAG,CAAC,IAAI,CAAC,GAAG,CAAC,CAAA;IACtB,CAAC,CAAC,CAAA;IACF,WAAW;IACX,MAAM