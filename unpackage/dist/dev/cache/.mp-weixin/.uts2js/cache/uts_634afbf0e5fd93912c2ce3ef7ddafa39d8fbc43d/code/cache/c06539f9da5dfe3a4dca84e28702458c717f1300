{"code":"import { __read } from \"tslib\";\nimport { computed, toRef } from 'vue';\nimport { useComponentColor, useComponentSize, useNamespace, } from '../../../../hooks';\nimport { formatDomSizeValue } from '../../../../utils';\nexport const useEmptyCustomStyle = (props, customIconContent, customTipsContent) => {\n    const ns = useNamespace('empty');\n    // 解析颜色\n    const _a = __read(useComponentColor(toRef(props, 'color'), 'text'), 2), textColorClass = _a[0], textColorStyle = _a[1];\n    const sizeType = useComponentSize(props.size).sizeType;\n    // empty对应的类\n    const emptyClass = computed(() => {\n        const cls = [ns.b()];\n        // 设置背景颜色\n        if (textColorClass.value)\n            cls.push(textColorClass.value);\n        // 设置尺寸\n        if (props.size && sizeType.value === 'inner')\n            cls.push(ns.m(props.size));\n        return cls.join(' ');\n    });\n    // empty对应的样式\n    const emptyStyle = computed(() => {\n        const style = {};\n        // 设置背景颜色\n        if (!textColorClass.value) {\n            style.color = textColorStyle.value || 'var(--tn-color-gray-disbaled)';\n        }\n        return style;\n    });\n    // 设置icon和text的样式\n    const iconTextStyle = computed(() => {\n        return (type) => {\n            const style = {};\n            // 设置用户自定义尺寸\n            if (props.size && sizeType.value === 'custom') {\n                if (type === 'icon' && !customIconContent.value) {\n                    style.fontSize = formatDomSizeValue(props.size);\n                    style.width = style.height = formatDomSizeValue(props.size);\n                }\n                if (type === 'tips' && !customTipsContent.value) {\n                    style.fontSize = `calc(${formatDomSizeValue(props.size)} * 0.35)`;\n                }\n            }\n            return style;\n        };\n    });\n    return {\n        ns,\n        emptyClass,\n        emptyStyle,\n        iconTextStyle,\n    };\n};\n//# sourceMappingURL=D:/Development/