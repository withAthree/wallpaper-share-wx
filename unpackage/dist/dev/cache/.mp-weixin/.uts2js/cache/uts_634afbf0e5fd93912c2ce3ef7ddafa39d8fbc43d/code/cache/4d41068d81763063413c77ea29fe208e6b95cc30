{"code":"import { computed, nextTick, ref, watch } from 'vue';\nimport { CHANGE_EVENT, UPDATE_MODEL_EVENT } from '../../../../constants';\nimport { isEmptyVariableInDefault } from '../../../../utils';\nexport const useSwiper = (props, emits) => {\n    // 当前选中的SwiperItem索引\n    const currentSwiperIndex = ref(isEmptyVariableInDefault(props === null || props === void 0 ? void 0 : props.modelValue, 0));\n    watch(() => { return props.modelValue; }, (value) => { return (currentSwiperIndex.value = isEmptyVariableInDefault(value, 0)); });\n    // 轮播数据\n    const swiperData = computed(() => {\n        var _a;\n        if ((_a = props.data) === null || _a === void 0 ? void 0 : _a.length)\n            return props.data;\n        if (props.blankCount)\n            return Array.from({ length: props.blankCount }).map((_, i) => { return i; });\n        return [];\n    });\n    // swiper数量\n    const swiperCount = computed(() => { var _a; return ((_a = swiperData.value) === null || _a === void 0 ? void 0 : _a.length) || 0; });\n    // 处理swiper滑动事件\n    const swiperChangeHandle = (event) => {\n        const current = event.detail.current;\n        if (props.modelValue === undefined || props.modelValue === 0)\n            currentSwiperIndex.value = current;\n        emits(UPDATE_MODEL_EVENT, current);\n        nextTick(() => {\n            emits(CHANGE_EVENT, current);\n        });\n    };\n    // 处理item点击事件\n    const itemClickHandle = () => {\n        emits('item-click', currentSwiperIndex.value);\n    };\n    return {\n        swiperData,\n        currentSwiperIndex,\n        swiperCount,\n        swiperChangeHandle,\n        itemClickHandle,\n    };\n};\n//# sourceMappingURL=D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/swiper/src/composables/use-swiper.js.map","references":["D:/Development/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3