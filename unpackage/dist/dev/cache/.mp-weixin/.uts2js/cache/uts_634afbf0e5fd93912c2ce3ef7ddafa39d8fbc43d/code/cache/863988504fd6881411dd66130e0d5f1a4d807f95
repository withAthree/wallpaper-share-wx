{"code":"import { ref } from 'vue';\nexport const useBubbleBox = (props, emits) => {\n    // 显示气泡弹出框\n    const showBubble = ref(false);\n    const openBubbleOptions = () => {\n        emits('open');\n        showBubble.value = true;\n    };\n    // 关闭气泡弹出框\n    const closeBubbleOptions = () => {\n        emits('close');\n        showBubble.value = false;\n    };\n    // 气泡框选项点击事件\n    const bubbleOptionClickEvent = (item, index) => {\n        if (props.disabled || item.disabled)\n            return;\n        emits('click', index);\n        if (props.autoClose)\n            closeBubbleOptions();\n    };\n    return {\n        showBubble,\n        openBubbleOptions,\n        closeBubbleOptions,\n        bubbleOptionClickEvent,\n    };\n};\n//# sourceMappingURL=D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/bubble-box/src/composables/use-bubble-box.js.map","references":["D:/Development/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/Development/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/bubble-box/src/bubble-box.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/bubble-box/src/types.ts"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"use-bubble-box.js\",\"sourceRoot\":\"\",\"sources\":[\"use-bubble-box.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,GAAG,EAAE,MAAM,KAAK,CAAA;AAMzB,MAAM,CAAC,MAAM,YAAY,GAAG,CAC1B,KAAqB,EACrB,KAA2C;IAE3C,UAAU;IACV,MAAM,UAAU,GAAG,GAAG,CAAU,KAAK,CAAC,CAAA;IAEtC,MAAM,iBAAiB,GAAG;QACxB,KAAK,CAAC,MAAM,CAAC,CAAA;QACb,UAAU,CAAC,KAAK,GAAG,IAAI,CAAA;IACzB,CAAC,CAAA;IAED,UAAU;IACV,MAAM,kBAAkB,GAAG;QACzB,KAAK,CAAC,OAAO,CAAC,CAAA;QACd,UAAU,CAAC,KAAK,GAAG,KAAK,CAAA;IAC1B,CAAC,CAAA;IAED,YAAY;IACZ,MAAM,sBAAsB,GAAG,CAC7B,IAA6B,EAC7B,KAAa;QAEb,IAAI,KAAK,CAAC,QAAQ,IAAI,IAAI,CAAC,QAAQ;YAAE,OAAM;QAC3C,KAAK,CAAC,OAAO,EAAE,KA