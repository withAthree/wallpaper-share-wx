{"code":"import { computed, inject, ref } from 'vue';\nimport { formContextKey } from '../../../../tokens';\nimport { formatDomSizeValue, generateId } from '../../../../utils';\nimport { useNamespace, useSelectorQuery } from '../../../../hooks';\nimport { useFormSize } from './use-form-common-props';\nexport const useFormItemCustomStyle = (props, hasLabel, isRequired) => {\n    const form = inject(formContextKey, undefined);\n    const ns = useNamespace('form-item');\n    const size = useFormSize(undefined, { formItem: false });\n    const getSelectorNodeInfo = useSelectorQuery().getSelectorNodeInfo;\n    // 标签的宽度\n    const labelWidth = computed(() => {\n        return formatDomSizeValue(props.labelWidth || (form === null || form === void 0 ? void 0 : form.labelWidth) || '');\n    });\n    // 标签的位置\n    const labelPosition = computed(() => { return props.labelPosition || (form === null || form === void 0 ? void 0 : form.labelPosition) || 'right'; });\n    // 是否隐藏必填星号\n    const hideRequiredAsterisk = computed(() => { return (form === null || form === void 0 ? void 0 : form.hideRequiredAsterisk) || false; });\n    // 必填星号的位置\n    const requireAsteriskPosition = computed(() => { return (form === null || form === void 0 ? void 0 : form.requireAsteriskPosition) || 'left'; });\n    // label标签容器宽度\n    const labelContainerWidth = ref(0);\n    const labelId = `label-${generateId()}`;\n    // 获取label标签的宽度\n    const initLabelContainerWidth = () => {\n        if (!hasLabel.value)\n            return;\n        getSelectorNodeInfo(`#${labelId}`).then((res) => {\n            labelContainerWidth.value = (res === null || res === void 0 ? void 0 : res.width) || 0;\n        });\n    };\n    // formItem所属类\n    const formItemClass = computed(() => {\n        const cls = [ns.b()];\n        if (size.value)\n            cls.push(ns.m(size.value));\n        if (labelPosition.value)\n            cls.push(ns.m(`label-${labelPosition.value}`));\n        return cls.join(' ');\n    });\n    // formItemLabel所属类\n    const formItemLabelClass = computed(() => {\n        const cls = [ns.e('label')];\n        if (!hideRequiredAsterisk.value && isRequired.value) {\n            cls.push(ns.em('label', 'required'), ns.em('label', `asterisk-${requireAsteriskPosition.value}`));\n        }\n        return cls.join(' ');\n    });\n    // formItemLabel所属样式\n    const formItemLabelStyle = computed(() => {\n        const style = {};\n        if (labelPosition.value !== 'top' && labelWidth.value)\n            style.width = labelWidth.value;\n        return style;\n    });\n    // formItemErrorMessage所属样式\n    const formItemErrorMessageStyle = computed(() => {\n        const style = {};\n        if (labelPosition.value !== 'top' && hasLabel.value) {\n            style.paddingLeft = `${labelContainerWidth.value}px`;\n        }\n        return style;\n    });\n    return {\n        ns,\n        labelId,\n        formItemClass,\n        formItemLabelClass,\n        formItemLabelStyle,\n        formItemErrorMessageStyle,\n        initLabelContainerWidth,\n    };\n};\n//# sourceMappingURL=D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/form/src/composables/form-item-custom.js.map","references":["D:/Development/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/tokens/index.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/utils/index.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/hooks/index.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/form/src/composables/use-form-common-props.ts","D:/Development/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/form/src/form-item.ts"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"form-item-custom.js\",\"sourceRoot\":\"\",\"sources\":[\"form-item-custom.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,QAAQ,EAAE,MAAM,EAAE,GAAG,EAAE,MAAM,KAAK,CAAA;AAC3C,OAAO,EAAE,cAAc,EAAE,MAAM,oBAAoB,CAAA;AACnD,OAAO,EAAE,kBAAkB,EAAE,UAAU,EAAE,MAAM,mBAAmB,CAAA;AAClE,OAAO,EAAE,YAAY,EAAE,gBAAgB,EAAE,MAAM,mBAAmB,CAAA;AAClE,OAAO,EAAE,WAAW,EAAE,MAAM,yBAAyB,CAAA;AAKrD,MAAM,CAAC,MAAM,sBAAsB,GAAG,CACpC,KAAoB,EACpB,QAAsB,EACtB,UAAwB;IAExB,MAAM,IAAI,GAAG,MAAM,CAAC,cAAc,EAAE,SAAS,CAAC,CAAA;IAE9C,MAAM,EAAE,GAAG,YAAY,CAAC,WAAW,CAAC,CAAA;IAEpC,MAAM,IAAI,GAAG,WAAW,CAAC,SAAS,EAAE,EAAE,QAAQ,EAAE,KAAK,EAAE,CAAC,CAAA;IAEhD,MAAA,mBAAmB,GAAK,gBAAgB,EAAE,oBAAvB,CAAuB;IAElD,QAAQ;IACR,MAAM,UAAU,GAAG,QAAQ,CAAC;QAC1B,OAAA,kBAAkB,CAAC,KAAK,CAAC,UAAU,KAAI,IAAI,aAAJ,IAAI,uBAAJ,IAAI,CAAE,UAAU,CAAA,IAAI,EAAE,CAAC;IAA9D,CAA8D,CAC/D,CAAA;IAED,QAAQ;IACR,MAAM,aAAa,GAAG,QAAQ,CAC5B,QAAM,OAAA,KAAK,CAAC,aAAa,KAAI,IAAI,aAAJ,IAAI,uBAAJ,IAAI,CAAE,aAAa,CAAA,IAAI,OAAO,EAArD,CAAqD,CAC5D,CAAA;IAED,WAAW;IACX,MAAM,oBAAoB,GAAG,QAAQ,CACnC,QAAM,OAAA,CAAA,IAAI,aAAJ,IAAI,uBAAJ,IAAI,CAAE,oBAAoB,KAAI,KAAK,EAAnC,CAAmC,CAC1C,CAAA;IAED,UAAU;IACV,MAAM,uBAAuB,GAAG,QAAQ,CACtC,QAAM,OAAA,CAAA,IAAI,aAAJ,IAAI,uBAAJ,IAAI,CAAE,uBAAuB,KAAI,MAAM,EAAvC,CAAuC,CAC9C,CAAA;IAED,cAAc;IACd,MAAM,mBAAmB,GAAG,GAAG,CAAC,CAAC,CAAC,CAAA;IAClC,MAAM,OAAO,GAAG,SAAS,UAAU,EAAE,EAAE,CAAA;IAEvC,eAAe;IACf,MAAM,uBAAuB,GAAG;QAC9B,IAAI,CAAC,QAAQ,CAAC,KAAK;YAAE,OAAM;QAC3B,mBAAmB,CAAC,IAAI,OAAO,EAAE,CAAC,CAAC,IAAI,CAAC,CAAC,GAAG;YAC1C,mBAAmB,CAAC,KAAK,GAAG,CAAA,GAAG,aAAH,GAAG,uBAAH,GAAG,CAAE,KAAK,KAAI,CAAC,CAAA;QAC7C,CAAC,CAAC,CAAA;IACJ,CAAC,CAAA;IAED,cAAc;IACd,MAAM,aAAa,GAAG,QAAQ,CAAS;QACrC,MAAM,GAAG,GAAa,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAA;QAE9B,IAAI,IAAI,CAAC,KAAK;YAAE,GAAG,CAAC,IAAI,CAAC,EAAE,CAAC,CAAC,CAAC,IAAI,CAAC,KAAK,CAAC,CAAC,CAAA;QAE1C,IAAI,aAAa,CAAC,KAAK;YAAE,GAAG,CAAC,IAAI,CAAC,EAAE,CAAC,CAAC,CAAC,SAAS,aAAa,CAAC,KAAK,EAAE,CAAC,CAAC,CAAA;QAEvE,OAAO,GAAG,CAAC,IAAI,CAAC,GAAG,CAAC,CAAA;IACtB,CAAC,CAAC,CAAA;IAEF,mBAAmB;IACnB,MAAM,kBAAkB,GAAG,QAAQ,CAAS;QAC1C,MAAM,