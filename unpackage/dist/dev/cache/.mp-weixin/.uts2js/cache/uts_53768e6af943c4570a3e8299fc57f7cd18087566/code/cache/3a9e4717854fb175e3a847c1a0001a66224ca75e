{"code":"import { __read } from \"tslib\";\nimport { computed, toRef } from 'vue';\nimport { useComponentColor, useNamespace } from '../../../../hooks';\nimport { formatDomSizeValue } from '../../../../utils';\nexport const useNumberBoxCustomStyle = (props, inputValue) => {\n    const ns = useNamespace('number-box');\n    // 解析颜色\n    const _a = __read(useComponentColor(toRef(props, 'bgColor'), 'bg'), 2), bgColorClass = _a[0], bgColorStyle = _a[1];\n    const _b = __read(useComponentColor(toRef(props, 'textColor'), 'text'), 2), textColorClass = _b[0], textColorStyle = _b[1];\n    // 步进器对应的类\n    const numberBoxClass = computed(() => {\n        const cls = [ns.b()];\n        // 设置尺寸\n        if (props.size)\n            cls.push(ns.m(props.size));\n        // 是否禁止操作\n        if (props.disabled)\n            cls.push(ns.m('disabled'));\n        return cls.join(' ');\n    });\n    // 步进器对应样式\n    const numberBoxStyle = computed(() => {\n        const style = {};\n        // 设置容器的宽高\n        if (props.width)\n            style.width = formatDomSizeValue(props.width);\n        if (props.height)\n            style.height = formatDomSizeValue(props.height);\n        // 设置字体大小\n        if (props.fontSize)\n            style.fontSize = formatDomSizeValue(props.fontSize);\n        return style;\n    });\n    // 步进器操作区域对应的类\n    const numberBoxOperationWrapperClass = computed(() => {\n        return (type) => {\n            const cls = [];\n            // 设置背景颜色和字体颜色\n            if (bgColorClass.value)\n                cls.push(bgColorClass.value);\n            if (textColorClass.value)\n                cls.push(textColorClass.value);\n            if ((type === 'minus' && inputValue.value <= props.min) ||\n                (type === 'plus' && inputValue.value >= props.max)) {\n                cls.push(ns.is('disabled'));\n            }\n            return cls.join(' ');\n        };\n    });\n    // 步进器操作区域对应的样式\n    const numberBoxOperationWrapperStyle = computed(() => {\n        return (type) => {\n            const style = {};\n            // 设置背景颜色和字体颜色\n            if (!bgColorClass.value)\n                style.backgroundColor =\n                    bgColorStyle.value || 'var(--tn-color-gray-light)';\n            if (textColorStyle.value)\n                style.color = textColorStyle.value;\n            // 设置操作按钮的宽高\n            if (type === 'minus' || type === 'plus') {\n                if (props.height) {\n                    style.width = formatDomSizeValue(props.height);\n                    style.height = style.width;\n                }\n                if (props.fontSize) {\n                    style.fontSize = `calc(${formatDomSizeValue(props.fontSize)} * 1.2)`;\n                }\n            }\n            return style;\n        };\n    });\n    return {\n        ns,\n        numberBoxClass,\n        numberBoxStyle,\n        numberBoxOperationWrapperClass,\n        numberBoxOperationWrapperStyle,\n    };\n};\n//# sourceMappingURL=D:/DEV/Project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/number-box/src/composables/number-box-custom.js.map","references":["D:/DEV/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/DEV/Project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/hooks/index.ts","D:/DEV/Project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/utils/index.ts","D:/DEV/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/DEV/Project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/number-box/src/number-box.ts"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"number-box-custom.js\",\"sourceRoot\":\"\",\"sources\":[\"number-box-custom.ts\"],\"names\":[],\"mappings\":\";AAAA,OAAO,EAAE,QAAQ,EAAE,KAAK,EAAE,MAAM,KAAK,CAAA;AACrC,OAAO,EAAE,iBAAiB,EAAE,YAAY,EAAE,MAAM,mBAAmB,CAAA;AACnE,OAAO,EAAE,kBAAkB,EAAE,MAAM,mBAAmB,CAAA;AAStD,MAAM,CAAC,MAAM,uBAAuB,GAAG,CACrC,KAAqB,EACrB,UAAuB;IAEvB,MAAM,EAAE,GAAG,YAAY,CAAC,YAAY,CAAC,CAAA;IAErC,OAAO;