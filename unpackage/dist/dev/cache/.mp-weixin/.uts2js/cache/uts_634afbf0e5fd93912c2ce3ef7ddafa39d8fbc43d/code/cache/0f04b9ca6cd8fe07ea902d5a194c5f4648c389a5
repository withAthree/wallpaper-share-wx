{"code":"import { isObject } from '@vue/shared';\nimport { debounce } from './debounce';\nconst FUNC_ERROR_TEXT = 'Expected a function';\nexport function throttle(func, wait, options) {\n    let leading = true, trailing = true;\n    if (typeof func != 'function') {\n        throw new TypeError(FUNC_ERROR_TEXT);\n    }\n    if (isObject(options)) {\n        leading = 'leading' in options ? !!options.leading : leading;\n        trailing = 'trailing' in options ? !!options.trailing : trailing;\n    }\n    return debounce(func, wait, {\n        leading,\n        maxWait: wait,\n        trailing,\n    });\n}\n//# sourceMappingURL=D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/libs/lodash/throttle.js.map","references":["D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/libs/lodash/debounce.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/libs/lodash/_common.ts"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"throttle.js\",\"sourceRoot\":\"\",\"sources\":[\"throttle.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,QAAQ,EAAE,MAAM,aAAa,CAAA;AACtC,OAAO,EAAE,QAAQ,EAAE,MAAM,YAAY,CAAA;AAIrC,MAAM,eAAe,GAAG,qBAAqB,CAAA;AAE7C,MAAM,UAAU,QAAQ,CACtB,IAAO,EACP,IAAa,EACb,OAA0B;IAE1B,IAAI,OAAO,GAAG,IAAI,EAChB,QAAQ,GAAG,IAAI,CAAA;IAEjB,IAAI,OAAO,IAAI,IAAI,UAAU,EAAE;QAC7B,MAAM,IAAI,SAAS,CAAC,eAAe,CAAC,CAAA;KACrC;IACD,IAAI,QAAQ,CAAC,OAAO,CAAC,EAAE;QACrB,OAAO,GAAG,SAAS,IAAI,OAAO,CAAC,CAAC,CAAC,CAAC,CAAC,OAAO,CAAC,OAAO,CAAC,CAAC,CAAC,OAAO,CAAA;QAC5D,QAAQ,GAAG,UAAU,IAAI,OAAO,CAAC,CAAC,CAAC,CAAC,CAAC,OAAO,CAAC,QAAQ,CAAC,CAAC,CAAC,QAAQ,CAAA;KACjE;IACD,OAAO,QAAQ,CAAC,IAAI,EAAE,IAAI,EAAE;QAC1B,OAAO;QACP,OAAO,EAAE,IAAI;QACb,QAAQ;KACT,CAAC,CAAA;AACJ,CAAC\"}"}
