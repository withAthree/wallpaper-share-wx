{"code":"import rules from '../rule';\nimport { isEmptyValue } from '../util';\nconst type = (rule, value, callback, source, options) => {\n    const ruleType = rule.type;\n    const errors = [];\n    const validate = rule.required || (!rule.required && source.hasOwnProperty(rule.field));\n    if (validate) {\n        if (isEmptyValue(value, ruleType) && !rule.required) {\n            return callback();\n        }\n        rules.required(rule, value, source, errors, options, ruleType);\n        if (!isEmptyValue(value, ruleType)) {\n            rules.type(rule, value, source, errors, options);\n        }\n    }\n    callback(errors);\n};\nexport default type;\n//# sourceMappingURL=D:/DEV/Project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/libs/async-validator/validator/type.js.map","references":["D:/DEV/Project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/libs/async-validator/interface.ts","D:/DEV/Project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/libs/async-validator/rule/index.ts","D:/DEV/Project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/libs/async-validator/util.ts"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"type.js\",\"sourceRoot\":\"\",\"sources\":[\"type.ts\"],\"names\":[],\"mappings\":\"AACA,OAAO,KAAK,MAAM,SAAS,CAAC;AAC5B,OAAO,EAAE,YAAY,EAAE,MAAM,SAAS,CAAC;AAEvC,MAAM,IAAI,GAAqB,CAAC,IAAI,EAAE,KAAK,EAAE,QAAQ,EAAE,MAAM,EAAE,OAAO;IACpE,MAAM,QAAQ,GAAG,IAAI,CAAC,IAAI,CAAC;IAC3B,MAAM,MAAM,GAAa,EAAE,CAAC;IAC5B,MAAM,QAAQ,GACZ,IAAI,CAAC,QAAQ,IAAI,CAAC,CAAC,IAAI,CAAC,QAAQ,IAAI,MAAM,CAAC,cAAc,CAAC,IAAI,CAAC,KAAK,CAAC,CAAC,CAAC;IACzE,IAAI,QAAQ,EAAE;QACZ,IAAI,YAAY,CAAC,KAAK,EAAE,QAAQ,CAAC,IAAI,CAAC,IAAI,CAAC,QAAQ,EAAE;YACnD,OAAO,QAAQ,EAAE,CAAC;SACnB;QACD,KAAK,CAAC,QAAQ,CAAC,IAAI,EAAE,KAAK,EAAE,MAAM,EAAE,MAAM,EAAE,OAAO,EAAE,QAAQ,CAAC,CAAC;QAC/D,IAAI,CAAC,YAAY,CAAC,KAAK,EAAE,QAAQ,CAAC,EAAE;YAClC,KAAK,CAAC,IAAI,CAAC,IAAI,EAAE,KAAK,EAAE,MAAM,EAAE,MAAM,EAAE,OAAO,CAAC,CAAC;SAClD;KACF;IACD,QAAQ,CAAC,MAAM,CAAC,CAAC;AACnB,CAAC,CAAC;AAEF,eAAe,IAAI,CAAC\"}"}
