{"code":"import { __read } from \"tslib\";\nimport { computed, toRef } from 'vue';\nimport { useComponentColor, useNamespace } from '../../../../hooks';\nimport { formatDomSizeValue, isEmpty } from '../../../../utils';\nexport const useTagCustomStyle = (props) => {\n    // 命名空间\n    const ns = useNamespace('tag');\n    // 解析背景颜色\n    const _a = __read(useComponentColor(toRef(props, 'bgColor'), 'bg'), 2), bgColorClass = _a[0], bgColorStyle = _a[1];\n    // 解析字体颜色\n    const _b = __read(useComponentColor(toRef(props, 'textColor'), 'text'), 2), textColorClass = _b[0], textColorStyle = _b[1];\n    // 解析边框颜色\n    const _c = __read(useComponentColor(toRef(props, 'borderColor'), 'border'), 2), borderColorClass = _c[0], borderColorStyle = _c[1];\n    // 标签动态类\n    const tagClass = computed(() => {\n        const cls = [];\n        cls.push(ns.b());\n        // 设置标签尺寸\n        if (props.size)\n            cls.push(ns.m(props.size));\n        // 设置标签形状\n        if (props.shape)\n            cls.push(ns.m(props.shape));\n        // 设置按钮颜色类型\n        if (props.type)\n            cls.push(`tn-type-${props.type}_bg`);\n        // 设置背景颜色\n        if (bgColorClass.value)\n            cls.push(bgColorClass.value);\n        // 设置字体颜色\n        if (textColorClass.value)\n            cls.push(textColorClass.value);\n        // 设置边框信息\n        if (props.border) {\n            cls.push('tn-border');\n            // 设置边框颜色\n            if (borderColorClass.value)\n                cls.push(borderColorClass.value);\n        }\n        // 设置边框加粗\n        if (props.borderBold)\n            cls.push('tn-border-bold');\n        if (props.customClass)\n            cls.push(props.customClass);\n        return cls.join(' ');\n    });\n    // 标签样式\n    const tagStyle = computed(() => {\n        const style = {};\n        // 设置字体大小\n        if (props.fontSize)\n            style.fontSize = formatDomSizeValue(props.fontSize);\n        // 设置宽度\n        if (props.width)\n            style.width = formatDomSizeValue(props.width);\n        // 设置高度\n        if (props.height)\n            style.height = formatDomSizeValue(props.height);\n        // 设置背景颜色\n        if (bgColorStyle.value)\n            style.backgroundColor = bgColorStyle.value;\n        // 设置字体颜色\n        if (textColorStyle.value)\n            style.color = textColorStyle.value;\n        // 设置边框颜色\n        if (borderColorStyle.value)\n            style.borderColor = borderColorStyle.value;\n        if (!isEmpty(props.customStyle)) {\n            Object.assign(style, props.customStyle);\n        }\n        return style;\n    });\n    return {\n        tagStyle,\n        tagClass,\n    };\n};\n//# sourceMappingURL=D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/tag/src/composables/tag-custom.js.map","references":["D:/Development/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/hooks/index.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/utils/index.ts","D:/Development/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/tag/src/tag.ts"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"tag-custom.js\",\"sourceRoot\":\"\",\"sources\":[\"tag-custom.ts\"],\"names\":[],\"mappings\":\";AAAA,OAAO,EAAE,QAAQ,EAAE,KAAK,EAAE,MAAM,KAAK,CAAA;AACrC,OAAO,EAAE,iBAAiB,EAAE,YAAY,EAAE,MAAM,mBAAmB,CAAA;AACnE,OAAO,EAAE,kBAAkB,EAAE,OAAO,EAAE,MAAM,mBAAmB,CAAA;AAI/D,MAAM,CAAC,MAAM,iBAAiB,GAAG,CAAC,KAAe;IAC/C,OAAO;IACP,MAAM,EAAE,GAAG,YAAY,CAAC,KAAK,CAAC,CAAA;IAC9B,SAAS;IACH,MAAA,KAAA,OAA+B,iBAAiB,CACpD,KAAK,CAAC,KAAK,EAAE,SAAS,CAAC,EACvB,IAAI,CACL,IAAA,EAHM,YAAY,QAAA,EAAE,YAAY,QAGhC,CAAA;IAED,SAAS;IACH,MAAA,KAAA,OAAmC,iBAAiB,CACxD,KAAK,CAAC,KAAK,EAAE,WAAW,CAAC,EACzB,MAAM,CACP,IAAA,EAHM,cAAc,QAAA,EAAE,cAAc,QAGpC,CAAA;IACD,SAAS;IACH,MAAA,KAAA,OAAuC,iBAAiB,