{"code":"import { __read } from \"tslib\";\nimport { computed } from 'vue';\nimport { useComponentColor, useComponentSize, useNamespace, } from '../../../../hooks';\nimport { formatDomSizeValue } from '../../../../utils';\nimport { useAvatarProps } from './use-avatar-props';\nexport const useAvatarCustomStyle = (props, groupIndex, avatarWidth) => {\n    const ns = useNamespace('avatar');\n    const _a = useAvatarProps(props), type = _a.type, size = _a.size, shape = _a.shape, bgColor = _a.bgColor, border = _a.border, borderColor = _a.borderColor, shadow = _a.shadow, shadowColor = _a.shadowColor, avatarGap = _a.avatarGap;\n    // 解析背景颜色\n    const _b = __read(useComponentColor(bgColor, 'bg'), 2), bgColorClass = _b[0], bgColorStyle = _b[1];\n    // 解析边框颜色\n    const _c = __read(useComponentColor(borderColor, 'border'), 2), borderColorClass = _c[0], borderColorStyle = _c[1];\n    // 解析阴影颜色\n    const _d = __read(useComponentColor(shadowColor, 'shadow'), 1), shadowColorClass = _d[0];\n    // 解析头像尺寸\n    const sizeType = useComponentSize(size.value).sizeType;\n    // 头像动态类\n    const avatarClass = computed(() => {\n        const cls = [];\n        cls.push(ns.b());\n        // 设置头像颜色类型\n        if (type.value)\n            cls.push(`tn-type-${type.value}_bg`);\n        // 设置背景颜色\n        if (!type.value && bgColorClass.value)\n            cls.push(bgColorClass.value);\n        // 设置头像尺寸\n        if (sizeType.value === 'inner')\n            cls.push(ns.m(size.value));\n        // 设置头像形状\n        if (shape.value)\n            cls.push(ns.m(shape.value));\n        // 设置边框\n        if (border.value) {\n            cls.push('tn-border');\n            // 设置边框颜色\n            if (borderColorClass.value)\n                cls.push(borderColorClass.value);\n        }\n        // 设置阴影\n        if (shadow.value) {\n            cls.push('tn-shadow');\n            // 设置阴影颜色\n            if (shadowColorClass.value)\n                cls.push(shadowColorClass.value);\n        }\n        return cls.join(' ');\n    });\n    // 头像动态样式\n    const avatarStyle = computed(() => {\n        const style = {};\n        // 设置头像尺寸\n        if (sizeType.value === 'custom') {\n            style.width = formatDomSizeValue(size.value);\n            style.height = style.width;\n        }\n        // 设置背景颜色\n        if (bgColorStyle.value)\n            style.backgroundColor = bgColorStyle.value;\n        // 设置边框颜色\n        if (border.value && borderColorStyle.value)\n            style.borderColor = borderColorStyle.value;\n        // 如果是头像组，设置头像间距\n        if (groupIndex.value != -1) {\n            style.zIndex = groupIndex.value + 1;\n            // style.transform = `translateX(calc(${\n            //   (groupAvatarCount.value - groupIndex.value - 1) * 100\n            // }% * ${avatarGap.value}))`\n            if (groupIndex.value > 0) {\n                style.marginLeft = `calc(-${avatarWidth.value * avatarGap.value}px)`;\n            }\n            else {\n                style.marginLeft = '0px';\n            }\n        }\n        return style;\n    });\n    return {\n        ns,\n        avatarClass,\n        avatarStyle,\n    };\n};\n//# sourceMappingURL=D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/avatar/src/composables/avatar-custom.js.map","references":["D:/Development/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/hooks/index.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/utils/index.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/avatar/src/composables/use-avatar-props.ts","D:/Development/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/Development/project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/avatar/src/avatar.ts"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"avatar-custom.js\",\"sourceR