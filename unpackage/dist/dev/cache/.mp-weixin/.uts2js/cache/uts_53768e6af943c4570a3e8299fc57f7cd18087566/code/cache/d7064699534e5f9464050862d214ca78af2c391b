{"code":"import { computed } from 'vue';\nimport {} from '../../../../utils';\nexport const usePhotoAlbum = (props, emits) => {\n    // 相册图片数据\n    const imageData = computed(() => {\n        const maxLength = Math.min(props.data.length, props.max);\n        return props.data.slice(0, maxLength);\n    });\n    // 图片点击事件\n    const imageClickEvent = (index) => {\n        emits('click', index);\n        if (!props.preview)\n            return;\n        uni.previewImage({\n            urls: imageData.value,\n            current: index,\n        });\n    };\n    return {\n        imageData,\n        imageClickEvent,\n    };\n};\n//# sourceMappingURL=D:/DEV/Project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/photo-album/src/composables/use-photo-album.js.map","references":["D:/DEV/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/DEV/Project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/utils/index.ts","D:/DEV/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/DEV/Project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/photo-album/src/photo-album.ts"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"use-photo-album.js\",\"sourceRoot\":\"\",\"sources\":[\"use-photo-album.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,QAAQ,EAAE,MAAM,KAAK,CAAA;AAC9B,OAAO,EAAE,MAAM,mBAAmB,CAAA;AAKlC,MAAM,CAAC,MAAM,aAAa,GAAG,CAC3B,KAAsB,EACtB,KAA2C;IAE3C,SAAS;IACT,MAAM,SAAS,GAAG,QAAQ,CAAW;QACnC,MAAM,SAAS,GAAG,IAAI,CAAC,GAAG,CAAC,KAAK,CAAC,IAAI,CAAC,MAAM,EAAE,KAAK,CAAC,GAAG,CAAC,CAAA;QACxD,OAAO,KAAK,CAAC,IAAI,CAAC,KAAK,CAAC,CAAC,EAAE,SAAS,CAAC,CAAA;IACvC,CAAC,CAAC,CAAA;IAEF,SAAS;IACT,MAAM,eAAe,GAAG,CAAC,KAAa;QACpC,KAAK,CAAC,OAAO,EAAE,KAAK,CAAC,CAAA;QACrB,IAAI,CAAC,KAAK,CAAC,OAAO;YAAE,OAAM;QAC1B,GAAG,CAAC,YAAY,CAAC;YACf,IAAI,EAAE,SAAS,CAAC,KAAK;YACrB,OAAO,EAAE,KAAK;SACf,CAAC,CAAA;IACJ,CAAC,CAAA;IAED,OAAO;QACL,SAAS;QACT,eAAe;KAChB,CAAA;AACH,CAAC,CAAA\"}"}
