{"code":"import { computed, provide, reactive, toRefs } from 'vue';\nimport { noticeBarKey } from '../../../../tokens';\nexport const useNoticeBar = (props, emits) => {\n    // 显示通知栏\n    const showNoticeBar = computed(() => {\n        return props.show && !(props.autoHide && props.data.length === 0);\n    });\n    // 播放状态\n    const play = computed(() => { return !props.pause; });\n    // 通知栏点击事件\n    const click = (index) => {\n        emits('click', index);\n    };\n    // 左图标点击事件\n    const leftIconClick = () => {\n        emits('left-icon-click');\n    };\n    // 右图标点击事件\n    const rightIconClick = () => {\n        emits('right-icon-click');\n    };\n    provide(noticeBarKey, reactive(Object.assign(Object.assign({}, toRefs(props)), { play,\n        click })));\n    return {\n        showNoticeBar,\n        leftIconClick,\n        rightIconClick,\n    };\n};\n//# sourceMappingURL=D:/DEV/Project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/notice-bar/src/composables/use-notice-bar.js.map","references":["D:/DEV/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/DEV/Project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/tokens/index.ts","D:/DEV/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts","D:/DEV/Project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/components/notice-bar/src/notice-bar.ts"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"use-notice-bar.js\",\"sourceRoot\":\"\",\"sources\":[\"use-notice-bar.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,QAAQ,EAAE,OAAO,EAAE,QAAQ,EAAE,MAAM,EAAE,MAAM,KAAK,CAAA;AACzD,OAAO,EAAE,YAAY,EAAE,MAAM,oBAAoB,CAAA;AAKjD,MAAM,CAAC,MAAM,YAAY,GAAG,CAC1B,KAAqB,EACrB,KAA2C;IAE3C,QAAQ;IACR,MAAM,aAAa,GAAG,QAAQ,CAAC;QAC7B,OAAO,KAAK,CAAC,IAAI,IAAI,CAAC,CAAC,KAAK,CAAC,QAAQ,IAAI,KAAK,CAAC,IAAI,CAAC,MAAM,KAAK,CAAC,CAAC,CAAA;IACnE,CAAC,CAAC,CAAA;IAEF,OAAO;IACP,MAAM,IAAI,GAAG,QAAQ,CAAC,QAAM,OAAA,CAAC,KAAK,CAAC,KAAK,EAAZ,CAAY,CAAC,CAAA;IAEzC,UAAU;IAC