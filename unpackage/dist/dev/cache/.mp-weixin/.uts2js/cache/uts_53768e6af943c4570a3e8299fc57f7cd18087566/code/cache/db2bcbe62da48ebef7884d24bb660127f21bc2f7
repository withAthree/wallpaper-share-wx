{"code":"export const useLongPress = (event, enabled, longPressIntervel = 250) => {\n    // 长按判断定时器\n    let longPressTimer = null;\n    // 清除长按判断定时器\n    const clearLongPressTimer = () => {\n        if (longPressTimer) {\n            clearInterval(longPressTimer);\n            longPressTimer = null;\n        }\n    };\n    // 处理长按事件\n    const handleLongPressEvent = (...args) => {\n        if (enabled.value) {\n            event(...args);\n            clearLongPressTimer();\n            longPressTimer = setInterval(() => {\n                event(...args);\n            }, longPressIntervel);\n        }\n        else {\n            event(...args);\n        }\n    };\n    return {\n        handleLongPressEvent,\n        clearLongPressTimer,\n    };\n};\n//# sourceMappingURL=D:/DEV/Project/wallpaper-share-wx/uni_modules/tuniaoui-vue3/hooks/use-long-press/index.js.map","references":["D:/DEV/HBuilderX/plugins/uniapp-cli-vite/node_modules/@vue/runtime-core/dist/runtime-core.d.ts"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"index.js\",\"sourceRoot\":\"\",\"sources\":[\"index.ts\"],\"names\":[],\"mappings\":\"AAEA,MAAM,CAAC,MAAM,YAAY,GAAG,CAC1B,KAA2B,EAC3B,OAAqB,EACrB,iBAAiB,GAAG,GAAG;IAEvB,UAAU;IACV,IAAI,cAAc,GAAyC,IAAI,CAAA;IAE/D,YAAY;IACZ,MAAM,mBAAmB,GAAG;QAC1B,IAAI,cAAc,EAAE;YAClB,aAAa,CAAC,cAAc,CAAC,CAAA;YAC7B,cAAc,GAAG,IAAI,CAAA;SACtB;IACH,CAAC,CAAA;IAED,SAAS;IACT,MAAM,oBAAoB,GAAG,CAAC,GAAG,IAAO;QACtC,IAAI,OAAO,CAAC,KAAK,EAAE;YACjB,KAAK,CAAC,GAAG,IAAI,CAAC,CAAA;YACd,mBAAmB,EAAE,CAAA;YACrB,cAAc,GAAG,WAAW,CAAC;gBAC3B,KAAK,CAAC,GAAG,IAAI,CAAC,CAAA;YAChB,CAAC,EAAE,iBAAiB,CAAC,CAAA;SACtB;aAAM;YACL,KAAK,CAAC,GAAG,IAAI,CAAC,CAAA;SACf;IACH,CAAC,CAAA;IAED,OAAO;QACL,oBAAoB;QACpB,mBAAmB;KACpB,CAAA;AACH,CAAC,CAAA\"}"}
